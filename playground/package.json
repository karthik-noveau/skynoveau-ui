    "build:react/core": "npm --prefix ../package/react/core run build && echo 'âœ… [react/core] build completed'",
    "build:react/utils": "npm --prefix ../package/react/utils run build && echo 'âœ… [react/utils] build completed'",
    "watch:react/core": "echo 'ðŸ”Ž Watching [react/core]...' && chokidar \"../package/react/core/src/**/*\" \"../package/react/core/package.json\" --initial --verbose --debounce 500 -c \"npm run build:react/core\"",
    "watch:react/utils": "echo 'ðŸ”Ž Watching [react/utils]...' && chokidar \"../package/react/utils/src/**/*\" \"../package/react/utils/package.json\" --initial --verbose --debounce 500 -c \"npm run build:react/utils\"",
